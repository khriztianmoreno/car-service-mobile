{
    "extends": "airbnb",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-console": "off",
        "jsx-quotes": ["error", "prefer-double"],
        "no-underscore-dangle": ["error", { "allow": ["_id"] } ],
        "strict": "off"
    }
}
